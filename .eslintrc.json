{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:github/es6",
    "plugin:pretter/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier", "jest"]
}
